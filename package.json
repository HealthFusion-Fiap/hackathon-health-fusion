{
  "name": "hackathon-health-fusion",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "npm i && tsc && tsc-alias && cp package*.json dist/ && npm install --production --prefix dist && cp -r prisma dist/ && npx prisma generate --schema=./prisma/schema.prisma --output=./dist/prisma",
    "lint": "eslint --ext .ts src",
    "test": "jest",
    "test:coverage": "npm test -- --coverage",
    "migrate": "npx prisma migrate dev",
    "start": "npx --prefix dist prisma generate --schema=./dist/prisma/schema.prisma && node dist/src/infra/api/server.js",
    "dev": "npm i && npm run migrate && nodemon -r tsconfig-paths/register src/infra/api/server.ts"
  },
  "author": "Team Fusion",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-sns": "^3.651.1",
    "@aws-sdk/client-ssm": "^3.651.1",
    "@prisma/client": "^5.19.1",
    "ajv": "^8.17.1",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^2.1.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.2.5",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.13",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.5.5",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "jest-mock-extended": "^4.0.0-beta1",
    "nodemon": "^3.1.7",
    "prisma": "^5.19.1",
    "ts-node": "^10.9.2"
  }
}